@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">View Data , View Bag , Temp Data</h1>
    <p>ViewBag:@ViewBag.ad</p>
    @foreach(var item in ViewBag.liste)
    {
        <p>Liste Elemanı: @item</p>

    }
    <p>@ViewBag.sonuc</p>
    <p>ViewData:@ViewData["soyad"]</p>
    <p>TempData:@TempData["cinsiyet"]</p>
</div>

  
